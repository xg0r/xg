#! /bin/bash/
roj='\033[1;31m'
ro2='\033[91m'
azu='\033[34m'

fin='\e[0m'
axxu="$(tput setaf 4)"
zzu='\e[4m'
echo " 

                           $roj       @@@@@@@ $fin
         ^^                $roj    @@@@@@@@@@@@ $fin
       ^^       ^^         $roj @@@@@@@@@@@@@@@@@@ $fin      $fin         ^^ $axxu
 ~~~~ ~~ ~~~~~ ~~~~~~~~ ~~ $ro2&&&&&&&&&&&&&&&&&&&&$axxu ~~~~~~~ ~~~~~~~~~~~ ~~~
 ~         ~~   ~  ~       ~~~~~~~~~~~~~~~~~~~~ ~       ~~     ~~ ~
   ~      ~~      ~~ ~~ ~~  ~~~~~~~~~~~~~ ~~~~  ~     ~~~    ~ ~~~  ~ ~~
   ~  ~~     ~         ~      ~~          ~~   ~~  ~~ ~~~       ~~ ~ ~~  ~~ ~
 ~  ~       ~ ~      ~           ~~ ~~       ~~~~  ~      ~~  ~             ~~
       ~             ~        ~      ~      ~~   ~             ~
$fin"